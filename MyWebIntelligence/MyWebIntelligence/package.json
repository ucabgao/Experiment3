{
  "name": "MyWebIntelligence",
  "version": "0.0.0",
  "description": "Understanding the web around you",
  "main": "index.js",
  "engines": {
    "node": ">=0.10.3 <0.12"
  },
  "scripts": {
    "postinstall": "cp git-hooks/pre-commit .git/hooks/pre-commit; chmod a+x .git/hooks/pre-commit",
    "test": "mocha test/automated/database/* test/automated/graph/test-cases/* test/automated/*",
    "start": "node server/index.js",
    "build-db-container": "docker build --tag=\"mywi/database\" ./postgresDB",
    "build-test-container": "docker build --tag=\"mywi/test\" .",
    "start-test-db-container": "docker rm mywi-test-db; docker run --name mywi-test-db -p 5600:5432 -e POSTGRES_PASSWORD=password -d mywi/database",
    "start-test": "docker run -p=6666:3333 -e \"NODE_ENV=test\" --link mywi-test-db:mywi-test-db mywi/test node server/index.js",
    "build-dev": "docker-compose -f compose-dev.yml build",
    "up-dev": "docker-compose -f compose-dev.yml up",
    "build-experimental": "docker-compose -f compose-experimental.yml build",
    "up-experimental": "docker-compose -f compose-experimental.yml up -d",
    "build-stable-container": "docker build --tag=\"mywi/stable\" .",
    "start-stable-db-container": "docker rm mywi-stable-db; docker run --name mywi-stable-db -p 5300:5432 -e POSTGRES_PASSWORD=password -d mywi/database",
    "start-stable": "docker run -d -p=3333:3333 -e \"NODE_ENV=stable\" --link mywi-stable-db:mywi-stable-db mywi/stable node server/index.js",
    "bundle": "jsx node_modules/react-tabs-component/ client/components/external/ Tabs && browserify client/index.js -o client/browserify-bundle.js -d",
    "watch": "watchify client/index.js -o client/browserify-bundle.js -d -v",
    "lint": "eslint ./",
    "build-test-crawl-docker-image": "docker build --tag=\"mywi/test-crawl\" --file=test/automated/crawl/Dockerfile .",
    "test-crawl": "docker run --add-host a.web:127.0.0.1 mywi/test-crawl /usr/bin/supervisord -c /usr/mywi/test/automated/crawl/supervisord.conf",
    "sql-generate": "node-sql-generate --dsn postgres://postgres:password@db:5432/postgres > ./postgresDB/declarations.js",
    "create-sql-tables": "node tools/createSQLTables.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MyWebIntelligence/MyWebIntelligence.git"
  },
  "keywords": [
    "web",
    "crawling",
    "SNA",
    "Social",
    "Networks",
    "text",
    "analysis"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MyWebIntelligence/MyWebIntelligence/issues"
  },
  "dependencies": {
    "better-log": "^1.2.0",
    "body-parser": "^1.10.1",
    "browserify": "~8.0.3",
    "compression": "~1.3.0",
    "csv-parser": "^1.6.0",
    "es6-promisify": "^1.1.1",
    "es6-shim": "^0.32.2",
    "es6-weak-map": "^1.0.2",
    "express": "~4.10.7",
    "express-session": "~1.10.1",
    "fast-csv": "^0.6.0",
    "google-pagerank": "0.0.3",
    "immutable": "^3.7.4",
    "ioredis": "^1.7.4",
    "jsdom": "3.x",
    "moment": "^2.10.2",
    "multer": "^0.1.7",
    "page": "^1.6.3",
    "passport": "~0.2.1",
    "passport-google-oauth": "~0.1.5",
    "pg": "^4.3.0",
    "react": "~0.12.2",
    "react-tabs-component": "^1.1.3",
    "react-tools": "^0.13.2",
    "request": "^2.53.0",
    "sigma": "^1.0.3",
    "simple-statistics": "^0.9.2",
    "sql": "^0.52.0",
    "sql-generate": "^1.0.0",
    "stringmap": "^0.2.2",
    "through": "^2.3.7",
    "tldjs": "^1.5.3",
    "tmp": "0.0.26"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "chai-as-promised": "^4.1.1",
    "eslint": "^0.24.1",
    "head": "^1.0.0",
    "mocha": "^2.1.0",
    "mock-require": "^1.0.5",
    "watchify": "~2.2.1"
  }
}
